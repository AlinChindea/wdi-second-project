<%- include ../partials/_header %>

<main class="container">
  <h3>All routes</h3>
  <div class="row">
  <% trips.forEach((trip) => { %>
    <ul class="col-lg-4">
    <li>
    <a href="/trips/<%= trip.id %>"><h3><%= trip.name %></h3></a>
    <img src="<%= trip.image %>" alt="<%= trip.name %>" class="img-thumbnail">
    <h4>River: <%= trip.river %></h4>
    </li>
    </ul>
    <% }) %>
  </div>
</main>

<%- include ../partials/_footer %>
